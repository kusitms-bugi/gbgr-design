{
	"name": "@gbgr/ui",
	"version": "0.0.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": [
		"**/*.css"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./styles.css": "./src/styles.css"
	},
	"dependencies": {
		"@gbgr/css": "workspace:*",
		"@gbgr/icons": "workspace:*",
		"@gbgr/react": "workspace:*"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18"
	},
	"devDependencies": {
		"react": "latest",
		"react-dom": "latest"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"clean": "rm -rf dist"
	}
}
