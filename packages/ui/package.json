{
	"name": "@gbgr/ui",
	"version": "0.1.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": [
		"**/*.css"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./accordion.css": "./src/accordion.css",
		"./button.css": "./src/button.css",
		"./checkbox.css": "./src/checkbox.css",
		"./mode-toggle.css": "./src/mode-toggle.css",
		"./text-field.css": "./src/text-field.css",
		"./styles.css": "./src/styles.css"
	},
	"dependencies": {
		"@gbgr/css": "workspace:*",
		"@gbgr/icons": "workspace:*",
		"@gbgr/react": "workspace:*"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18"
	},
	"devDependencies": {
		"react": "latest",
		"react-dom": "latest"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"clean": "rm -rf dist"
	}
}
